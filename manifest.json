{
  "manifest_version": 3,
  "version": "6.6.6",
  "name": "Off Youtube Bullshit",
  "description": "Disables or removes unnecessary YouTube elements.",
  "content_scripts": [{
    "matches": ["*://*.youtube.com/*"],
    "css": ["style.css"],
    "js": ["content.js"],
    "run_at": "document_end",
    "all_frames": true
  }],
  "icons": {
    "256": "icon.png"
  }
}
